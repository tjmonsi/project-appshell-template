{
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "CustomElements": false,
    "HTMLImports": false,
    "Polymer": false,
    "WeakMap": false
  },
  "plugins": [ "html" ],
  "settings": {
    "html/indent": "0",   
    "html/indent": "+2",  
    "html/indent": "tab", 
  }
}